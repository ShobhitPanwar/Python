class Solution:
    def trailingZeroes(self, N):
        if (N<5):
            return 0;
        return N//5 + self.trailingZeroes(N//5)
if __name__ == '__main__':
    t = int(input())
    for _ in range (t):
        N = int(input())
        ob = Solution()
        ans = ob.trailingZeroes(N)
        print (ans)
